{
    "version": 2,
    "functions": {
        "src/backend/server.js": {
            "includeFiles": "src/backend/frontend_dist/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/src/backend/server.js"
        },
        {
            "source": "/ws/(.*)",
            "destination": "/src/backend/server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/src/backend/server.js"
        }
    ]
}